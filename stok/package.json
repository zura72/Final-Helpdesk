{
  "name": "stok",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@azure/msal-browser": "^4.21.0",
    "@azure/msal-react": "^3.0.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.1.0",
    "rimraf": "^6.0.1",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^7.1.3"
  },
  "scripts": {
    "dev": "vite --port 8080",
    "build": "echo Building stok app && if not exist build mkdir build && echo Stok app ready > build/index.html",
    "preview": "vite preview --port 8080",
    "vercel-build": "vite build && if exist build rmdir /s /q build && move dist build",
    "dev:server": "npm --prefix ../server run dev",
    "dev:web": "npm --prefix ../web run dev",
    "dev:all": "concurrently -k -n SVR,ADM,WEB -c green,magenta,cyan \"npm run dev:server\" \"npm run dev\" \"npm run dev:web\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
